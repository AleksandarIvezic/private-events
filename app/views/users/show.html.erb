<h1> Created Events</h1>
<ul>
<% @events.each do |event|%>

    <li><%= event.title%></li>
    <li><%= event.details%></li>

<% end %>

</ul>


<h1>Your attending events</h1>

<% current_user.attended_events.each do |event|%>
    <h3><%= event.title%></h3>
    <p><%= event.details%></p>
    <p><%= event.event_date%></p>
<% end %>